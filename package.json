{
  "name": "nio-eval",
  "version": "0.0.0",
  "description": "Evaluation for n.io",
  "dependencies": {
    "d3": "^3.5.13",
    "niojs": "^1.1.5"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "http-server": "^0.8.5",
    "less": "^2.5.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "uglifyify": "^3.0.1",
    "watchify": "^3.7.0"
  },
  "scripts": {
    "js": "mkdir -p public/js && browserify ./src/app.js -t uglifyify -o public/js/bundle.js",
    "js:watch": "mkdir -p public/js && watchify ./src/app.js -t uglifyify -o public/js/bundle.js -v",
    "js:lint": "eslint src",
    "css": "mkdir -p public/css && lessc less/app.less --autoprefix=\"last 2 versions\" --clean-css public/css/bundle.css",
    "server": "http-server -p 4000 public/"
  },
  "author": "James Holmes <32bitkid@gmail.com> (http://refactorman.com)",
  "private": true,
  "license": "UNLICENSED"
}
